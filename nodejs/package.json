{
  "name": "@fynal/aikit",
  "version": "1.0.0",
  "description": "Fynal Development Kit",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "build": "tsc; mkdir -p node_modules/fynal; cp build/index.* node_modules/fynal/; echo \"declare module 'fynal';\" | cat - build/index.d.ts > temp && mv temp node_modules/fynal/index.d.ts; tsc -p ./tsconfig_examples.json",
    "prepublishOnly": "pnpm version minor",
    "publish": "npm publish --registry=https://registry.npmjs.org --access public"
  },
  "keywords": [],
  "author": "liukehong@gmail.com",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.14.12",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
  },
  "dependencies": {
    "@hapi/code": "^9.0.3",
    "axios": "^1.7.2"
  }
}
